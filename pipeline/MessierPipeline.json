{
	"name": "MessierPipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy Messier Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"ID"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['fields']['french_name_nom_francais']"
								},
								"sink": {
									"name": "FrenchName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['latin_name_nom_latin']"
								},
								"sink": {
									"name": "LatinName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['mag']"
								},
								"sink": {
									"name": "Magnitude",
									"type": "Byte"
								}
							},
							{
								"source": {
									"path": "['fields']['messier']"
								},
								"sink": {
									"name": "ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['english_name_nom_en_anglais']"
								},
								"sink": {
									"name": "EnglishName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['dec']"
								},
								"sink": {
									"name": "Declination",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['ra']"
								},
								"sink": {
									"name": "RightAscention",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['id']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['mimetype']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['height']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['width']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['filename']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['thumbnail']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['format']"
								}
							},
							{
								"source": {
									"path": "['fields']['annee']"
								}
							},
							{
								"source": {
									"path": "['fields']['image']"
								}
							},
							{
								"source": {
									"path": "['record_timestamp']"
								}
							},
							{
								"source": {
									"path": "$['nhits']"
								}
							},
							{
								"source": {
									"path": "$['parameters']['dataset']"
								}
							},
							{
								"source": {
									"path": "$['parameters']['rows']"
								}
							},
							{
								"source": {
									"path": "$['parameters']['start']"
								}
							},
							{
								"source": {
									"path": "$['parameters']['format']"
								}
							},
							{
								"source": {
									"path": "$['parameters']['timezone']"
								}
							},
							{
								"source": {
									"path": "['fields']['image_url']['color_summary'][0]"
								}
							}
						],
						"collectionReference": "$['records']"
					}
				},
				"inputs": [
					{
						"referenceName": "MessierRESTResource",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "MessierSQLTable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}